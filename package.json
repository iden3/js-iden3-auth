{
  "name": "js-iden3-auth",
  "version": "0.1.1",
  "description": "iden3-auth implementation in JavaScript",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/mjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist/*.js",
    "dist/**/*.js"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && ./fixup",
    "buildcli": "rollup -c config/rollup.cli.config.js",
    "buildiife": "BROWSER=true rollup -c config/rollup.iife.config.js",
    "buildiifemin": "BROWSER=true rollup -c config/rollup.iife_min.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint --ext *ts"
  },
  "bin": {
    "snarkjs": "build/cli.cjs"
  },
  "directories": {
    "templates": "templates"
  },
  "keywords": [
    "iden3"
  ],
  "author": "iden3",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/iden3/js-iden3-auth"
  },
  "dependencies": {
    "base58-js": "^1.0.0",
    "bigint-buffer": "^1.1.5",
    "bs58": "^5.0.0",
    "crypto-js": "^4.1.1",
    "ethers": "^5.4.0",
    "snarkjs": "^0.4.16"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.13.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^27.2.5",
    "@types/jest": "27.4.1",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
