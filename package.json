  "description": "iden3-auth implementation in JavaScript",
  "main": "dist/cjs/index.js",
  "source": "./src/index.ts",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
