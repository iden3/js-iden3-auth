{
  "name": "js-iden3-auth",
  "type": "module",
  "version": "0.1.0",
  "description": "iden3-auth implementation in JavaScript",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/mjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && ./fixup",
    "buildcli": "rollup -c config/rollup.cli.config.js",
    "buildiife": "BROWSER=true rollup -c config/rollup.iife.config.js",
    "buildiifemin": "BROWSER=true rollup -c config/rollup.iife_min.config.js",
    "lint": "eslint --ext *js"
  },
  "bin": {
    "snarkjs": "build/cli.cjs"
  },
  "directories": {
    "templates": "templates"
  },
  "keywords": [
    "iden3"
  ],
  "author": "iden3",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/iden3/js-iden3-auth"
  },
  "dependencies": {
    "base58-js": "^1.0.0",
    "bigint-buffer": "^1.1.5",
    "bs58": "^5.0.0",
    "crypto-js": "^4.1.1",
    "ethers": "^5.4.0",
    "snarkjs": "^0.4.16"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.13.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^28.0.3",
    "@types/node": "^14.17.4",
    "typescript": "^4.3.5"
  }
}
