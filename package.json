    "@iden3/js-jwz": "1.0.0-beta.1",
    "@iden3/js-merkletree": "1.0.0-beta.0",
    "axios": "^0.27.2",
    "base58-js": "^1.0.0",
    "cross-sha256": "^1.2.0",
    "ethers": "^5.4.0",
    "ipfs-http-client": "^56.0.3",
    "jsonld": "^8.1.1",
    "keccak256": "^1.0.6",
    "nested-property": "^4.0.0",
